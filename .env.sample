TELEGRAM_BOT_TOKEN=
WINDSURF_WINDOW_TITLE=Windsurf
GEMINI_API_KEY=

# Optional remote controller (if used by bot)
REMOTE_CONTROLLER_URL=

### Core Automation
RESPONSE_WAIT_SECONDS=15.0
RESPONSE_MAX_WAIT_SECONDS=300.0
RESPONSE_POLL_INTERVAL_SECONDS=10.0
RESPONSE_STABLE_MIN_SECONDS=5.0
PASTE_RETRY_COUNT=2
COPY_RETRY_COUNT=2
KEY_DELAY_SECONDS=0.2
USE_APPLESCRIPT_ON_MAC=1
FRONTMOST_WAIT_SECONDS=3.0

### Visual Region (used for right panel bounds)
VISUAL_REGION_TOP=100
VISUAL_REGION_BOTTOM=150

### Echo Filter & Copy Fallback
ECHO_FILTER_ENABLED=1
ECHO_PREFIX_LEN=24
ECHO_MAX_DELTA=64
ECHO_LEN_RATIO=1.4
USE_COPY_SHORT_FALLBACK=1

### Ready Pixel (главный триггер)
USE_READY_PIXEL=1
READY_PIXEL_X=1243
READY_PIXEL_Y=713
READY_PIXEL_R=122
READY_PIXEL_G=120
READY_PIXEL_B=113
READY_PIXEL_TOL=12
READY_PIXEL_TOL_PCT=15.0
READY_PIXEL_REQUIRED=1

# Координатный режим и сдвиги
READY_PIXEL_COORD_MODE=top
READY_PIXEL_DX=0
READY_PIXEL_DY=0
READY_PIXEL_PROBE_INTERVAL_SECONDS=7.0

### Answer/Input focus points
# INPUT_ABS_X/Y — точный клик в поле ввода перед вставкой
INPUT_ABS_X=1050
INPUT_ABS_Y=725
# ANSWER_ABS_X/Y — клик в область ответа (используется как fallback и при копировании)
ANSWER_ABS_X=1087
ANSWER_ABS_Y=349
# COPY_CLICK_X/Y — вспомогательный клик перед копированием (после скролла вниз)
COPY_CLICK_X=1256
COPY_CLICK_Y=675

# COPY_DRAG_START_X/Y — начальная точка протяжки перед копированием
COPY_DRAG_START_X=1245
COPY_DRAG_START_Y=604
# COPY_DRAG_END_X/Y — конечная точка протяжки
COPY_DRAG_END_X=913
COPY_DRAG_END_Y=100
# COPY_DRAG_HOLD_SECONDS — удержание мыши на конце протяжки
COPY_DRAG_HOLD_SECONDS=5.0

### Debugging
SAVE_VISUAL_DEBUG=0
SAVE_VISUAL_SAMPLES=0
SAVE_READY_ONLY_ON_MATCH=0
SAVE_READY_HYPOTHESES=0
SAVE_VISUAL_DIR=debug

# Обрезать ответ после вашего вопроса (всегда брать часть, идущую после запроса)
TRIM_AFTER_PROMPT=1
LOG_LEVEL=DEBUG

# WSModel confirm probe (палитра Cmd+/): точка проверки перед кликом подтверждения
# Если пиксель белый (>=254 по всем каналам), клик смещается на безопасную точку
WSMODEL_PROBE_X=1179
WSMODEL_PROBE_Y=728

# Безопасная точка клика подтверждения при белом фоне в палитре
WSMODEL_CONFIRM_SAFE_X=1130
WSMODEL_CONFIRM_SAFE_Y=695

# Финальная проверка пикселя после /change перед безопасным кликом
CHANGE_FINAL_PROBE_X=1205
CHANGE_FINAL_PROBE_Y=15